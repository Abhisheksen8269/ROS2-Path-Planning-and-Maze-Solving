<robot name="maze_bot">

    <!-- <link name="base_footprint"/> -->
    <link name="base">
      
      <inertial>
        <mass value="5" />
        <origin xyz="0 0 0" rpy="0 0 0" />
        <inertia ixx="0.004" ixy="0.001" ixz="0" iyy="0.006" iyz="0" izz="0.007" />
      </inertial>

      <visual>
        <geometry>
            <mesh filename="package://maze_bot/meshes/base_link.stl"/>
            <!-- <box size="0.75 0.4 0.1"/> -->
        </geometry>
        <material name="gray">
          <color rgba=".2 .2 .2 1" />
        </material>
      </visual>
  
      <collision>
        <geometry>
            <box size="0.75 0.4 0.1"/>
        </geometry>
        <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      </collision>

      
    </link>
    <!-- <joint name="base_joint" type="fixed">
      <parent link="base_footprint"/>
      <child link="base"/>
      <origin rpy="0 0 0" xyz="0 0 0.15 "/>
    </joint> -->


  <link name="wheel_right">
      <inertial>
        <mass value="1"/>
        <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        <origin/>
      </inertial>
  
      <visual>
        
        <geometry>
            <mesh filename="package://maze_bot/meshes/wheel.stl"/>
            <!-- <box size="0.75 0.4 0.1"/> -->
        </geometry>

        <material name="white">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
  
      <collision>
      
        <geometry>
            <cylinder radius="0.15" length="0.1"/>
        </geometry>
        <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      </collision>
    </link>
  
    <joint name="wheel_right_joint" type="continuous">
      <origin xyz="0.4 0.36 0.17" rpy="0 0.0 0.0"/>
      <parent link="base"/>
      <child link="wheel_right"/>
      <axis xyz="0.0 1.0 0"/>
    </joint>
  
    <link name="wheel_left">
      <inertial>
        <mass value="1"/>
        <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        <origin/>
      </inertial>
  
      <visual>
        
        <geometry>
            <mesh filename="package://maze_bot/meshes/wheel.stl"/>
            <!-- <box size="0.75 0.4 0.1"/> -->
        </geometry>
        
        <material name="white">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
  
      <collision>
        <geometry>
            <mesh filename="package://maze_bot/meshes/wheel.stl"/>
            <!-- <box size="0.75 0.4 0.1"/> -->
        </geometry>
        <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      </collision>
    </link>
  
    <joint name="wheel_left_joint" type="continuous">
      <origin xyz="0.4 -0.43 0.17" rpy="0 0.0 3.14"/>
      <parent link="base"/>
      <child link="wheel_left"/>
      <axis xyz="0.0 1 0"/>
  
    </joint>
  
  
  
    <link name="caster">
      <inertial>
        <mass value="1"/>
        <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        <origin/>
      </inertial>
  
      <visual>
        <geometry>
            <mesh filename="package://maze_bot/meshes/front_caster.stl"/>
            <!-- <box size="0.75 0.4 0.1"/> -->
        </geometry>
        <material name="white" />
      </visual>
  
      <collision>
        <origin/>
        <geometry>
          <sphere radius=".02" />
        </geometry>
        <contact_coefficients mu="0" kp="1000.0" kd="1.0"/>
      </collision>
    </link>
  
    <joint name="caster_joint" type="continuous">
      <origin xyz="-0.62 -0.03 -0.05" rpy="0.0 0.0 0.0"/>
      <axis xyz="0 0 1" />
      <parent link="base"/>
      <child link="caster"/>
    </joint> 
  
  </robot>
  